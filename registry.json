{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "registry demo",
  "homepage": "",
  "items": [
    {
      "$schema": "https://ui.shadcn.com/schema/registry-item.json",
      "name": "start",
      "type": "registry:theme",
      "title": "Fluent Starting Kit",
      "description": "A blank app with the Fluent theme preconfigured.",
      "files": [
        {
          "path": "tailwind.config.ts",
          "type": "registry:file",
          "target": "tailwind.config.ts"
        },
        {
          "path": "app/globals.css",
          "type": "registry:file",
          "target": "app/globals.css"
        },
        {
          "path": "app/(fluent)/layout.tsx",
          "type": "registry:file",
          "target": "app/layout.tsx"
        }
      ]
    },
    {
      "$schema": "https://ui.shadcn.com/schema/registry-item.json",
      "name": "dashboard",
      "type": "registry:component",
      "title": "Dashboard",
      "description": "A complete dashboard implementation with sidebar, stats cards, charts, and tables.",
      "registryDependencies": [
        "https://registry-demo-xi.vercel.app/r/start.json"
      ],
      "files": [
        {
          "path": "app/(fluent)/dashboard/page.tsx",
          "type": "registry:file",
          "target": "app/dashboard/page.tsx"
        },
        {
          "path": "components/dashboard/app-sidebar.tsx",
          "type": "registry:file",
          "target": "components/dashboard/app-sidebar.tsx"
        },
        {
          "path": "components/dashboard/branch-switcher.tsx",
          "type": "registry:file",
          "target": "components/dashboard/branch-switcher.tsx"
        },
        {
          "path": "components/dashboard/chart-section.tsx",
          "type": "registry:file",
          "target": "components/dashboard/chart-section.tsx"
        },
        {
          "path": "components/dashboard/department-table.tsx",
          "type": "registry:file",
          "target": "components/dashboard/department-table.tsx"
        },
        {
          "path": "components/dashboard/header.tsx",
          "type": "registry:file",
          "target": "components/dashboard/header.tsx"
        },
        {
          "path": "components/dashboard/recent-activity.tsx",
          "type": "registry:file",
          "target": "components/dashboard/recent-activity.tsx"
        },
        {
          "path": "components/dashboard/search-form.tsx",
          "type": "registry:file",
          "target": "components/dashboard/search-form.tsx"
        },
        {
          "path": "components/dashboard/stats-cards.tsx",
          "type": "registry:file",
          "target": "components/dashboard/stats-cards.tsx"
        }
      ]
    }
  ]
}
